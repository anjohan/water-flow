tolerance 2.0

filetype xyz
output data/withwater.xyz


nloop 10000

structure data/passivert.xyz
    number 1
    fixed 0. 0. 0. 0. 0. 0.
end structure

structure water.xyz
    number NUMBEROFMOLECULES
    outside cylinder MIDx MIDy WATERBOTTOM 0 0 1 RADIUS HEIGHT
    inside box 0 0 WATERBOTTOM X Y WATERTOP
end structure
